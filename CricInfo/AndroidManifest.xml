<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.jaf.cricinfo.feeds"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/cricket" android:label="@string/app_name">
        <activity android:name="com.jaf.cricinfo.feeds.CricInfoRss"
                  android:label="@string/app_name"   android:theme="@android:style/Theme.Dialog" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
 <activity android:name="com.jaf.cricinfo.feeds.GetRSSFeeds"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.jaf.cricinfo.GET_FEEDS" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.jaf.cricinfo.feeds.OtherTeams"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.jaf.cricinfo.OTHER_TEAMS" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity android:name="com.jaf.cricinfo.feeds.ViewNews"
                  android:label="@string/app_name"  >
            <intent-filter>
                <action android:name="com.jaf.cricinfo.VIEW_PAGE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
    <uses-sdk android:minSdkVersion="4" />

<uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest> 